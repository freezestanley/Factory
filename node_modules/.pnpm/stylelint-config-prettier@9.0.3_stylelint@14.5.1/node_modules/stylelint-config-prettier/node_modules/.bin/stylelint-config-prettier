#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-config-prettier@9.0.3_stylelint@14.5.1/node_modules/stylelint-config-prettier/bin/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-config-prettier@9.0.3_stylelint@14.5.1/node_modules/stylelint-config-prettier/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-config-prettier@9.0.3_stylelint@14.5.1/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules:/Users/stanleyxu/Documents/work/facefactory/node_modules:/Users/stanleyxu/Documents/work/node_modules:/Users/stanleyxu/Documents/node_modules:/Users/stanleyxu/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-config-prettier@9.0.3_stylelint@14.5.1/node_modules/stylelint-config-prettier/bin/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-config-prettier@9.0.3_stylelint@14.5.1/node_modules/stylelint-config-prettier/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-config-prettier@9.0.3_stylelint@14.5.1/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules:/Users/stanleyxu/Documents/work/facefactory/node_modules:/Users/stanleyxu/Documents/work/node_modules:/Users/stanleyxu/Documents/node_modules:/Users/stanleyxu/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/check.js" "$@"
else
  exec node  "$basedir/../../bin/check.js" "$@"
fi
