#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint@14.5.1/node_modules/stylelint/bin/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint@14.5.1/node_modules/stylelint/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint@14.5.1/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules:/Users/stanleyxu/Documents/work/facefactory/node_modules:/Users/stanleyxu/Documents/work/node_modules:/Users/stanleyxu/Documents/node_modules:/Users/stanleyxu/node_modules:/Users/node_modules:/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-prettier@2.0.0_prettier@2.5.1+stylelint@14.5.1/node_modules/stylelint-prettier/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-prettier@2.0.0_prettier@2.5.1+stylelint@14.5.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint@14.5.1/node_modules/stylelint/bin/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint@14.5.1/node_modules/stylelint/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint@14.5.1/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules:/Users/stanleyxu/Documents/work/facefactory/node_modules:/Users/stanleyxu/Documents/work/node_modules:/Users/stanleyxu/Documents/node_modules:/Users/stanleyxu/node_modules:/Users/node_modules:/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-prettier@2.0.0_prettier@2.5.1+stylelint@14.5.1/node_modules/stylelint-prettier/node_modules:/Users/stanleyxu/Documents/work/facefactory/Factory/node_modules/.pnpm/stylelint-prettier@2.0.0_prettier@2.5.1+stylelint@14.5.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint@14.5.1/node_modules/stylelint/bin/stylelint.js" "$@"
else
  exec node  "$basedir/../../../../../stylelint@14.5.1/node_modules/stylelint/bin/stylelint.js" "$@"
fi
